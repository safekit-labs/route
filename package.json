{
  "name": "@safekit/route",
  "version": "0.1.0",
  "author": "safekit",
  "repository": {
    "type": "git",
    "url": "https://github.com/safekit-labs/route.git"
  },
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js"
    }
  },
  "bugs": {
    "url": "https://github.com/safekit-labs/route/issues"
  },
  "description": "A type-safe url builder with full type safety and runtime validation",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "homepage": "https://github.com/safekit-labs/route",
  "keywords": [
    "typescript",
    "route",
    "link",
    "path",
    "safe",
    "safekit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "build": "bun clean && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "dev:typecheck": "bun typecheck --watch --preserveWatchOutput",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "vitest",
    "ci": "bun run lint && bun run typecheck && bun run build && bun run test --run",
    "git": "bun run ci && git add . && git commit -m",
    "postgit": "git push --all"
  },
  "devDependencies": {
    "@effect/schema": "^0.75.5",
    "@swc/cli": "*",
    "@swc/core": "*",
    "@types/node": "^24.0.4",
    "@types/qs": "^6.14.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "arktype": "^2.1.20",
    "effect": "^3.17.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.3.0",
    "jiti": "^2.4.2",
    "next-safe-action": "^8.0.7",
    "prettier": "^3.6.2",
    "runtypes": "^7.0.4",
    "superstruct": "^2.0.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "typia": "^9.3.1",
    "valibot": "^1.1.0",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "yup": "^1.6.1",
    "zod": "^4.0.8"
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "dependencies": {
    "qs": "^6.14.0"
  }
}
